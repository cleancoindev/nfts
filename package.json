    "next": "12.0.9",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.6",
    "npm-run-all": "^4.1.5"
  }
